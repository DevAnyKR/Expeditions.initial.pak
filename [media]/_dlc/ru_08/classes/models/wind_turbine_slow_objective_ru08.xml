<ModelBrand
	DynamicModel="true"
	NightLightingShadowsAllowCap="true"
	
>
	<Subset IsDefault="true" Name="build_stage_0">
		<Mesh Name="stage_0" />
		<CdtMesh Name="stage_0" />
	</Subset>

	<Subset Name="build_complete">
		<OnSubsetActivateEvent>
		
			<Sound Name="actors/actor_construction/actor_construct_us_lumbermill_stuff_03" Range="50" Volume="1" />

		</OnSubsetActivateEvent>
		<Mesh Name="stage_1" />
		<CdtMesh Name="stage_1" />
		<AnimTrackSubset Name="anim_loop" />
	</Subset>

	<PhysicsModel>
		<Body Collisions="Dynamic" ModelFrame="stage_0">
			<Body Collisions="Dynamic" ModelFrame="stage_1" />
		</Body>
	</PhysicsModel>

	<Landmark Mesh="landmarks/wind_turbine_ru08_lmk" />
	
</ModelBrand>