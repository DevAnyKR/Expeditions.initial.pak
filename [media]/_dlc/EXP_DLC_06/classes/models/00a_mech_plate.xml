<_templates Include="models" />
<ModelBrand IsKeepPhysModelAlways="true">
	<PhysicsModel>
		<Body
			Collisions="Dynamic"
			Mass="1000"
			ModelFrame="BonePlate_cdt"
			NoSoftContacts="True"
			CenterOfMassOffset="(0; 0; 0)"
		>
			<Constraint
				AxisLocal="(0; 0; 1)"
				Type="Prismatic"
				MinLimit="0"
				MaxLimit="1.5"
			>
				<Motor Damping="3000" Spring="1500" Type="Spring" />
			</Constraint>
		</Body>
	</PhysicsModel>
	<GameData>
		<WinchSocket Pos="(0; 1.5; 1.2)" ParentFrame="BonePlate_cdt" />
	</GameData>
</ModelBrand>


