<CombineParticles
	AirResistance="10.0"
	AspectRatio="1.4"

	MudDecals="0.0"
	Wind="(0.0; 0.0; 0.0)"
	SoftDepth="0.1"
	OverdrawDegradation="true"
	SortingPriority="2"
	BillboardOffset="(0.0; -1.7)"
>
	<Texture
		Diffuse="sfx/particles/rolling_dust__d_a.tga"
		Distort="sfx/substance/distort__n_d.tga"
		DistortMult="0.8"
		DistortScale="0.2"
		DistortSpeed="0.05"
		RollEnd="0.6"
		RollStart="0.8"
		RollingMask="sfx/particles/rolling_smoke_mask_small__s_d.tga"
	/>

	<Size Function="(0.0; 0.3; 0.3)" Median="0.5" />
	<Scale Random="0.5" Value="0.6" />
	<RotationSpeed Random="4" />
	<Rotation Random="360" />
	<LifeTime Random="0.250" Value="0.75" />
	<Color>
		<Key At="0.0" Value="g(10; 10; 10; 200)" />
		<Key At="0.2" Value="g(137; 122; 97;  100)" />
		<Key At="0.7" Value="g(10; 10; 10;  60)" />
		<Key At="1.0" Value="g(137; 122; 97;  1)" />
	</Color>
	<Collisions
		Enable="true"
		AffectedByIntake="true"
		
		Friction="0.9"
		Radius="0.02"
		Restitution="0.9"
	/>
</CombineParticles>
